{
    "extends": [
        "tslint:recommended", 
        "tslint-react", 
        "tslint-config-prettier"
    ],
    "rules":{
        "no-console":false,
        "no-debugger":false,
        "indent":[true, "spaces", 4],
        "no-consecutive-blank-lines": [
            true,
            2
        ],
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}